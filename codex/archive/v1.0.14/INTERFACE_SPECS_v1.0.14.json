{
  "schemas": {
    "ActionProposal": {
      "type": "object",
      "required": [
        "id",
        "timestamp",
        "context",
        "stakeholders",
        "proposal",
        "domain_config_sha256"
      ],
      "properties": {
        "id": {
          "type": "string"
        },
        "timestamp": {
          "type": "string"
        },
        "context": {
          "type": "object"
        },
        "stakeholders": {
          "type": "array",
          "items": {
            "type": "string"
          }
        },
        "proposal": {
          "type": "string"
        },
        "domain_config_sha256": {
          "type": "string"
        }
      }
    },
    "DecisionReport": {
      "type": "object",
      "required": [
        "action_id",
        "invariants",
        "quantitative",
        "agree",
        "drift",
        "decision",
        "rationale_ascii",
        "telemetry",
        "payload_sha256"
      ],
      "properties": {
        "action_id": {
          "type": "string"
        },
        "invariants": {
          "type": "object"
        },
        "quantitative": {
          "type": "object"
        },
        "agree": {
          "type": "object"
        },
        "drift": {
          "type": "object"
        },
        "decision": {
          "type": "string",
          "enum": [
            "PERMIT",
            "ABSTAIN",
            "BLOCK"
          ]
        },
        "rationale_ascii": {
          "type": "string",
          "maxLength": 800
        },
        "telemetry": {
          "type": "object"
        },
        "payload_sha256": {
          "type": "string"
        }
      }
    },
    "Telemetry": {
      "type": "object",
      "properties": {
        "fp": {
          "type": "integer"
        },
        "fn": {
          "type": "integer"
        },
        "abstain_rate": {
          "type": "number"
        },
        "agree_skipped_pct": {
          "type": "number"
        },
        "EDD_days": {
          "type": "number"
        },
        "FAR_pct_per_week": {
          "type": "number"
        },
        "TTD_hours": {
          "type": "number"
        },
        "pi_override_90d": {
          "type": "number"
        },
        "abstain_cost_z": {
          "type": "number"
        }
      }
    }
  },
  "adapters": {
    "AgreeAdapter_structured": "Jaccard",
    "AgreeAdapter_unstructured": "embedding_distance_normalized"
  }
}